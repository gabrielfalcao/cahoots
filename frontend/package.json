    "superagent": "5.2.2",
    "typescript": "3.8.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "tsc && react-scripts build",
    "watch": "tsc --watch",
    "format": "prettier --write **/*.tsx",
    "format-check": "prettier --check **/*.tsx",
    "test": "react-scripts test",
    "test-watch": "jest --runInBand --detectOpenHandles --bail --watch",
